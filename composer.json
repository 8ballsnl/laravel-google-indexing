{
    "name": "famdirksen/laravel-google-indexing",
    "description": "Index",
    "keywords": [
        "famdirksen",
        "laravel-google-indexing"
    ],
    "homepage": "https://github.com/famdirksen/laravel-google-indexing",
    "license": "MIT",
    "authors": [
        {
            "name": "Robin Dirksen",
            "email": "robin@famdirksen.nl",
            "homepage": "https://famdirksen.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.3",
        "phpunit/phpunit": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Famdirksen\\LaravelGoogleIndexing\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Famdirksen\\LaravelGoogleIndexing\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Famdirksen\\LaravelGoogleIndexing\\SkeletonServiceProvider"
            ],
            "aliases": {
                "Skeleton": "Famdirksen\\LaravelGoogleIndexing\\SkeletonFacade"
            }
        }
    }
}
